<%- include('../partials/header.ejs') %>
<%- include('../partials/navbar.ejs') %>


<h1 class="heading"> Update President</h1>
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="add_btn">
                <a href="/presidents"><i class="bi bi-bar-chart-fill"></i>Manage President</a>
            </div>
            <form method="post" action="/upgradepresident/<%- president._id %>">
                <input type="text" name="name" value="<%- president.name %>" />
                <input type="text" name="yearofadministration" value="<%- president.yearofadministration %>" />
                <button class="btn btn-success">Update</button>
            </form>
        </div>
    </div>
</div>










